{
  "name": "docland",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn start:dev",
    "start:dev": "webpack-dev-server --port 8090",
    "start:prod": "yarn build && yarn serve build -p 9090",
    "build": "NODE_ENV=production webpack",
    "build:clean": "rm -rf build",
    "lint": "yarn eslint . --ext .ts && tsc --noEmit",
    "fix": "prettier --write src && yarn eslint:fix",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@primer/css": "^17.5.2",
    "@types/jszip": "^3.4.1",
    "diff": "^5.0.0",
    "docland-readability": "^0.4.7",
    "jsonld": "^5.2.0",
    "jszip": "^3.6.0",
    "linki": "^0.1.16",
    "linki-ui": "^0.1.8",
    "pdfjs-dist": "^2.6.347",
    "posthog-js": "^1.13.5",
    "schema-dts": "^0.7.1"
  },
  "devDependencies": {
    "@peculiar/webcrypto": "^1.1.7",
    "@types/diff": "^5.0.0",
    "@types/jest": "^27.4.1",
    "@types/jsonld": "^1.5.2",
    "@types/mozilla-readability": "^0.2.0",
    "@types/pdfjs-dist": "^2.1.7",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@typescript-eslint/typescript-estree": "^4.33.0",
    "babel-jest": "^26.6.3",
    "copy-webpack-plugin": "^6.2.0",
    "css-loader": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unused-imports": "^1.1.1",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.5.1",
    "linki-ui-storybook": "^0.2.0",
    "prettier": "^2.2.1",
    "speed-measure-webpack-plugin": "^1.5.0",
    "storybook": "^6.4.9",
    "storybook-addon-turbo-build": "^1.0.1",
    "style-loader": "^1.2.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^8.0.4",
    "typescript": "^4.2.4",
    "webpack": "^5.51.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0",
    "whatwg-fetch": "^3.6.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "preset": "ts-jest/presets/js-with-ts-esm",
    "transform": {
      "node_modules/linki/.+\\.(j|t)s$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!linki/)"
    ],
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ]
  }
}
